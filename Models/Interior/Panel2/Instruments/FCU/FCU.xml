<?xml version="1.0"?>

<PropertyList>
	<path>FCU.ac</path>
	
	<effect>
		<inherits-from>../../Effects/lcd_display</inherits-from>
		<object-name>FCU.display</object-name>
	</effect>
	
	<animation>
		<type>pick</type>
		<object-name>throttle.knob</object-name>
		<action>
			<button>0</button>
			<repeatable>0</repeatable>
			<binding>
				<command>property-assign</command>
				<property>instrumentation/FCU/push-knob</property>
				<value>1</value>
			</binding>
			<binding>
				<command>nasal</command>
				<script>fes.push_knob(1);</script>
			</binding>
			<mod-up>
				<binding>
					<command>property-assign</command>
					<property>instrumentation/FCU/push-knob</property>
					<value>0</value>
				</binding>
				<binding>
					<command>nasal</command>
					<script>fes.push_knob(0);</script>
				</binding>
			</mod-up>
		</action>
	</animation>
	<animation>
		<type>translate</type>
		<object-name>throttle.knob</object-name>
		<property>instrumentation/FCU/push-knob</property>
		<factor>-0.001</factor>
		<axis>
			<x>1</x>
		</axis>
	</animation>
	<animation>
		<type>knob</type>
		<object-name>throttle.knob</object-name>
		<value>0</value>
		<interpolation>
			<entry><ind>0</ind><dep>0</dep></entry>
			<entry><ind>1.0</ind><dep>260</dep></entry>
		</interpolation>
		<center>
			<x-m>0.009</x-m>
			<y-m>0.0</y-m>
			<z-m>0.022</z-m>
		</center>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>systems/FES/throttle</property>
				<factor>0.05</factor>
				<min>-1.0</min>
				<max>1</max>
				<wrap>0</wrap>
			</binding>
		</action>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>fcu.switch</object-name>
		<property>systems/FES/fcu-switch</property>
		<factor>-30</factor>
		<center>
			<x-m>0.001931</x-m>
			<z-m>0.022423</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>fcu.switch</object-name>
		<action>
			<button>0</button>
			<binding>
				<command>property-toggle</command>
				<property>systems/FES/fcu-switch</property>
			</binding>
		</action>
	</animation>
	<animation>
		<type>material</type>
		<object-name>light.ok</object-name>
		<condition>
			<property>instrumentation/FCU/lights/ok-green</property>
		</condition>
		<ambient>
			<red>0.4</red>
			<green>0.4</green>
			<blue>0.4</blue>
